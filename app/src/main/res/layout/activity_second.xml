<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="360dp"
    android:layout_height="800dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginLeft="40dp"

    tools:ignore="ExtraText,RtlHardcoded">

    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="321dp"
        android:layout_height="251dp"
        android:padding="0dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="62dp">


        <ImageView
            android:id="@+id/imageView"
            android:layout_width="321dp"
            android:layout_height="251dp"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="507dp"
            app:srcCompat="@drawable/mainreg"
            tools:layout_editor_absoluteY="32dp" />

        <TextView
            android:id="@+id/text1"
            android:layout_width="210dp"
            android:layout_height="44dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="159dp"
            android:text="@string/get_started"

            android:textColor="#252525"
            android:textSize="36sp"
            app:layout_constraintStart_toStartOf="parent" />


        <TextView
            android:id="@+id/text2"
            android:layout_width="183dp"
            android:layout_height="17dp"
            android:layout_marginLeft="26dp"
            android:layout_marginTop="203dp"

            android:text=" by creating a free account"
            android:textColor="#252525"
            android:textSize="14dp"

            app:layout_constraintStart_toStartOf="parent" />

    </FrameLayout>

    <EditText
        android:id="@+id/fulname"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:ems="10"
        android:text=" Full name"
        android:drawableEnd="@drawable/person_24dp_5f6368_fill0_wght400_grad0_opsz24"
        android:paddingRight="10dp"
        android:inputType="text"
        android:paddingLeft="15dp"

        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/style"
        android:layout_marginTop="256dp"
        android:layout_marginLeft="30dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:paddingRight="5dp"
        android:id="@+id/editTextText2"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginTop="325dp"
        android:ems="10"
        android:drawableEnd="@drawable/mail_24dp_5f6368_fill0_wght400_grad0_opsz24"
        android:inputType="text"
        android:text="  Valid email"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
       android:layout_marginLeft="30dp"
        android:background="@drawable/style"/>

    <EditText
        android:paddingRight="5dp"
        android:id="@+id/editTextText3"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="text"
        android:text="  Phone number"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="394dp"
        android:layout_marginLeft="30dp"
        android:background="@drawable/style"
        android:drawableEnd="@drawable/phone_iphone_24dp_5f6368_fill0_wght400_grad0_opsz24"/>

    <EditText
        android:paddingRight="5dp"
        android:id="@+id/editTextText4"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="text"
        android:text="  Strong password"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
       android:layout_marginTop="463dp"
        android:layout_marginLeft="30dp"
        android:background="@drawable/style"
        android:drawableEnd="@drawable/lock_24dp_5f6368_fill0_wght400_grad0_opsz24"/>

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="12dp"
        android:layout_height="12dp"
        android:layout_marginTop="532dp"
        android:layout_marginLeft="12dp"
        android:buttonTint="@color/cbcbcb"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        android:background="@drawable/style"/>

    <TextView
        android:id="@+id/textlink2"
        android:layout_width="241dp"
        android:layout_height="10dp"
        android:layout_marginTop="532dp"
        android:layout_marginLeft="30dp"
        android:text="@string/link2"
        android:textSize="9dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


    <Button
        android:id="@+id/button2"
        android:layout_width="280dp"
        android:layout_height="60dp"
        android:layout_marginTop="660dp"
        android:text="@string/next"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/memberlogin"
        android:layout_width="176dp"
        android:layout_height="16dp"
        android:layout_marginTop="743dp"
        android:layout_marginLeft="83dp"
        android:text="@string/memberlink"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />  <!-- Makes URLs clickable -->



</androidx.constraintlayout.widget.ConstraintLayout>